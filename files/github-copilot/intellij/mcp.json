{
  "servers": {
    "com.atlassian/atlassian-mcp-server": {
      "type": "http",
      "url": "https://mcp.atlassian.com/v1/mcp",
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "com.atlassian/atlassian-mcp-server",
            "version": "1.1.1"
          }
        }
      }
    },
    "io.github.ChromeDevTools/chrome-devtools-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@0.15.0"
      ],
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "io.github.ChromeDevTools/chrome-devtools-mcp",
            "version": "0.15.0"
          }
        }
      }
    },
    "io.github.upstash/context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.31"
      ],
      "env": {
        "CONTEXT7_API_KEY": ""
      },
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "io.github.upstash/context7",
            "version": "1.0.31"
          }
        }
      }
    },
    "io.github.github/github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "requestInit": {
        "headers": {
          "Authorization": ""
        }
      },
      "x-metadata": {
        "registry": {
          "api": {
            "baseUrl": "https://api.mcp.github.com",
            "version": "v0.1"
          },
          "mcpServer": {
            "name": "io.github.github/github-mcp-server",
            "version": "0.30.2"
          }
        }
      }
    }
  }
}