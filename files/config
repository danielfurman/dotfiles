Host n1-centos
    HostName 192.168.121.46
    User vagrant
    Port 22
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    PasswordAuthentication no
    IdentityFile ~/.ssh/n1-centos
    IdentitiesOnly yes
    LogLevel FATAL
    ProxyCommand ssh -q 56 nc -q0 192.168.121.46 22
