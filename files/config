Host jenkins
    Hostname 10.100.0.129
    User ubuntu
    IdentityFile ~/.ssh/openstack

Host review.contrail.juniper.net
    ProxyCommand nc -x localhost:1080 %h %p

Host ssd-git.juniper.net
    ProxyCommand nc -x localhost:1080 %h %p

# Contrail test environments
Host 0.*
    Hostname 10.100.%h
    User centos
    IdentityFile ~/.ssh/kube_aws_rsa

Host 7
    HostName 10.84.27.7
    User root

Host 56
    HostName 10.87.69.56
    User root

Host n1-centos
  HostName 192.168.121.46
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/.ssh/n1-centos
  IdentitiesOnly yes
  LogLevel FATAL
  ProxyCommand ssh -q 56 nc -q0 192.168.121.46 22
