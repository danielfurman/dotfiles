# Hosts configuration for home RSA keys

# Fenthick github account
Host github.com
    HostName github.com
    IdentityFile ~/.ssh/id_rsa_fenthick

# daniel.furman@ntti3.com bitbucket account
Host bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/id_rsa_ntt

# fenthick92@gmail.com bitbucket account
Host fenthick.bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/id_rsa_fenthick

# Global controller
Host global
    Hostname        10.15.2.28
    IdentityFile    ~/.ssh/esi_deployment_key
    User            esi

# Local controller
Host local
    Hostname        10.15.2.61
    IdentityFile    ~/.ssh/esi_deployment_key
    User            esi

# Device in Palo Alto
Host device
    User            eseuser
    IdentityFile    ~/.ssh/vlanner_key
    ProxyCommand ssh -q local nc -q0 10.2.2.79 22

# Rome Global controller
Host global_rome
    Hostname        10.15.2.228
    IdentityFile    ~/.ssh/esi_deployment_key
    User            esi
